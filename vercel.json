{
    "buildCommand": "npm run build",
    "outputDirectory": "dist/odop-project",
    "framework": null,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://odop-backend.onrender.com/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}
